<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.0" name="Excel">
    <description>Provides a scanner for Excel files.</description>
    <model>
        <class>de.kontext_e.jqassistant.plugin.excel.store.descriptor.ExcelFileDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.excel.store.descriptor.ExcelSheetDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.excel.store.descriptor.ExcelRowDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.excel.store.descriptor.ExcelCellDescriptor</class>
    </model>
    <scanner>
        <class>de.kontext_e.jqassistant.plugin.excel.scanner.ExcelFileScannerPlugin</class>
    </scanner>

    <rules>
        <resource>excel.xml</resource>
    </rules>
</jqa-plugin:jqassistant-plugin>
